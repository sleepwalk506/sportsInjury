<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huali.sportsinjuryandrecovery.mapper.InjuryInquiryMapper">
    <select id="injuryInquiryList" resultType="com.huali.sportsinjuryandrecovery.pojo.Injury">
        select * from injury
        <where>
            injuryType=#{injuryType};
        </where>
    </select>
</mapper>